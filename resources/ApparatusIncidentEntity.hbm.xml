<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.jjanisch.ember.entity.ApparatusIncidentEntity" table="apparatus_incident" schema="emberims">
        <id name="apparatusIncidentId">
            <column name="apparatus_incident_id" sql-type="int(11)"/>
        </id>
        <property name="incidentId">
            <column name="incident_id" sql-type="int(11)"/>
        </property>
        <property name="apparatusId">
            <column name="apparatus_id" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>