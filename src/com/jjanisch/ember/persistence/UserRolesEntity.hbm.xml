<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.jjanisch.ember.persistence.UserRolesEntity" table="user_roles" schema="emberims">
        <composite-id mapped="true" class="com.jjanisch.ember.persistence.UserRolesEntityPK">
            <key-property name="userName">
                <column name="user_name" sql-type="varchar(15)" length="15"/>
            </key-property>
            <key-property name="roleName">
                <column name="role_name" sql-type="varchar(20)" length="20"/>
            </key-property>
        </composite-id>
    </class>
</hibernate-mapping>